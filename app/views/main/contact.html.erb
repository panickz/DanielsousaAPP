<div id="container">
    <div class="map">
        <iframe width="100%" height="350" frameborder="0" scrolling="no" marginheight="0" marginwidth="0" src="https://maps.google.com/maps?f=q&amp;source=s_q&amp;hl=it&amp;geocode=&amp;q=roma&amp;sll=37.0625,-95.677068&amp;sspn=60.158465,135.263672&amp;ie=UTF8&amp;hq=&amp;hnear=Roma,+Lazio,+Italia&amp;t=m&amp;z=11&amp;ll=41.892916,12.48252&amp;output=embed"></iframe>
    </div>

    <!-- CONTACT SECTION -->
    <section id="profile">
        <div class="content">
            <div class="two-third-first">

                <!-- FORM SECTION -->
                <form action="http://www.stefanoschintu.com/webfolio/send.php" name="contactform" class="contactform" method="post">
                    <label for="name">Full Name<span class="required">*</span></label>
                    <input type="text" name="name" id="name" placeholder="Enter your name" required="">
                    <label for="company">Company<span class="required">*</span></label>
                    <input type="text" name="company" id="company" placeholder="Where do you work?" required="">
                    <label for="email">Email<span class="required">*</span></label>
                    <input type="email" name="email" id="email" pattern="^[A-Za-z0-9](([_\.\-]?[a-zA-Z0-9]+)*)@([A-Za-z0-9]+)(([\.\-]?[a-zA-Z0-9]+)*)\.([A-Za-z]{2,})$" placeholder="e.g. info@envato.com" required="">
                    <div class="options">
                        <label id="budget1">Personal Options<span class="required">*</span></label>
                        <br>
                        <input type="radio" name="option" id="option1" value="10 to 1000" checked="">
                        <label for="option1" class="radio">$10 &nbsp;&nbsp;<i class="icon-hand-right green"></i>&nbsp;&nbsp; $1.000</label>
                        <input type="radio" name="option" id="option2" value="1001 to 5000">
                        <label for="option2" class="radio">$1.001 &nbsp;&nbsp;<i class="icon-hand-right green"></i>&nbsp;&nbsp; $5.000</label>
                        <input type="radio" name="option" id="option3" value="5001 to 10000+">
                        <label for="option3" class="radio">$5.001 &nbsp;&nbsp;<i class="icon-hand-right green"></i>&nbsp;&nbsp; $10.000+</label>
                    </div>
                    <label for="project">Project Description<span class="required">*</span></label>
                    <br>
                    <textarea name="project" id="project" cols="40" rows="5" placeholder="Describe your project and add some details." required=""></textarea>
                    <button type="submit" name="submit" class="bt-navy txt-right fx1">Send &nbsp;&nbsp;<i class="icon-circle-arrow-right green"></i></button>
                </form>
                <!-- FORM SECTION ENDS -->

            </div>
            <div class="one-third-last">
                <h3><%=@info.name%></h3>
                <p><i class="icon-phone navy"></i>&nbsp;&nbsp; <strong>+00 351 <%=@info.phone%></strong></p>
                <p><a href="#"><i class="icon-envelope navy"></i>&nbsp;&nbsp; <%=@info.email%></a></p>
            </div>
        </div>
    </section>
    <!-- CONTACT SECTION ENDS -->

    <!-- FOOTER SECTION -->
    <div id="footer">
        <div class="inner-footer">
            <div class="one-half-first copyright white">All Rights Reserved to Webfolio 2013</div>
            <div class="one-half-last txt-right"><a class="sc-icons" href="#"><i class="icon-twitter green"></i></a><a class="sc-icons" href="#"><i class="icon-facebook green"></i></a><a class="sc-icons" href="#"><i class="icon-pinterest green"></i></a><a class="sc-icons" href="#"><i class="icon-linkedin green"></i></a><a class="sc-icons" href="#"><i class="icon-flickr green"></i></a></div>
        </div>
    </div>
    <!-- #footer -->
</div>